/* Program 3: Prace s retezci a vestavenymi funkcemi */
function MyMain(str) // Hlavni telo programu
    str1 = str[:19.0]
    str2 = str1 + ", ktery je ulozeny jako "
    t = typeOf(str2)
    str2 = str2 + t
    x = print(str1, "\n", str2, "\n")
    p = find(str2, "text")
    x = print("Pozice retezce \"text\" v retezci str2: ", p, "\n")
    x = print("Zadejte nejakou posloupnost vsech malych pismen a-h, ")
    x = print("pricemz se pismena nesmeji v posloupnosti opakovat\n")
    str1 = input()
    str2 = sort(str1)
    while str2 != "abcdefgh"
    x = print("Spatne zadana posloupnost, zkuste znovu\n")
    str1 = input()
    str2 = sort(str1)
    end
    return 0.0
    end
    foo = MyMain("Toto je nejaky text v programu jazyka IFJ12")
